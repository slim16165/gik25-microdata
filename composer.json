{
  "name": "gik25/gik25-microdata",
  "description": "",
  "type": "wordpress-plugin",
  "minimum-stability": "dev",
  "license": "proprietary",
  "authors": [
    {
      "name": "Gianluigi Salvi",
      "email": "gianluigi.salvi@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "gik25microdata\\": "include/class/",
      "gik25microdata\\site_specific\\": "include/site_specific/"
    }
  },
  "require": {
    "athari/yalinqo": "^2.0",
    "yiisoft/html": "dev-master",
    "illuminate/collections": "8.*"
  },
  "config": {
    "allow-plugins": false
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/extension-installer": "^1.2.0",
    "szepeviktor/phpstan-wordpress": "dev-master",
    "vimeo/psalm": "^4.0"
  },
  "scripts": {
    "validate-syntax": [
      "bash scripts/validate-php-syntax.sh"
    ],
    "validate-syntax-windows": [
      "powershell -ExecutionPolicy Bypass -File scripts/validate-php-syntax.ps1"
    ]
  }
}
